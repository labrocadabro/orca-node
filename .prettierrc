{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
	"jsxSingleQuote": true,
	"semi": true,
	"trailingComma": "all",
	"printWidth": 100,
	"bracketSameLine": false,
	"arrowParens": "always",
	"endOfLine": "auto",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "overrides": [
    {
      "files": ".eslintrc",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
